            var MONS_LOG_VARS = {"JobID":198787001,"ApplyType":"4","PopUpMessage":"Ihr Popup-Blocker verhindert die korrekte Ausführung dieser Seite.","EE":"EAAQrXuZmYIUF5avSTEZs5tdZQ--","ApplyOnlineUrl":"https://CAREERS.SCHAEFFLER.COM/sap/bc/erecruiting/applwzd?PARAM=cG9zdF9pbnN0X2d1aWQ9QUEwRTVGNDJBNzAyMUVFODg0RTRFM0U0MkE3MDlGRDcmY2FuZF90eXBlPQ%3d%3d&sap-client=100","TrackingCompany":"LuK GmbH & Co. KG","TrackingLogo":"http://unternehmen.monster.de/profile/schaeffler-ag","YTA":0,"LogUrl":"LogUrl :'http://stellenanzeige.monster.de/log.ashx?PositionAdID=198787001&folderid=234351662&tracking=&logtype=","XCode":"xjp71360dex"};

            //for CAN
            var clickLogQueue = {
                registeredCallbacks: [],

                registerCallback: function (fn) {
                    this.registeredCallbacks.push(fn);
                },

                proccessQueue: function (logType, href) {
                    var i, len;
                    for (i = 0, len = this.registeredCallbacks.length; i < len; i += 1) {
                        this.registeredCallbacks[i](logType, href);
                    }
                }
            }
            function submitLog(log_src) {
                if (document.images) {
                    submitLog.log_img[submitLog.log_ptr] = new Image;
                    submitLog.log_img[submitLog.log_ptr].src = log_src;
                    submitLog.log_ptr++;
                }
            }
            submitLog.log_img = new Array;
